{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "rootDir": "src",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true},
      "strict": true,
      "skipLibCheck": true,
      "noImplicitAny": true,
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"],
    "typeRoots": ["./node_modules/@types", "./src/types"]
  }
  